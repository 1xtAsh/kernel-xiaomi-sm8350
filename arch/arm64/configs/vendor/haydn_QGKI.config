# Arch specific
CONFIG_LD_DEAD_CODE_DATA_ELIMINATION=y

# Fingerprint
CONFIG_FINGERPRINT_FPC_TEE=m

# General setup
# CONFIG_SLUB_CPU_PARTIAL is not set

# Haptics
CONFIG_INPUT_QCOM_HV_HAPTICS=m

# KernelSU
CONFIG_KSU=y

# Platform
CONFIG_MACH_XIAOMI_HAYDN=y

# Network
CONFIG_NET_SCH_FQ_CODEL=y
CONFIG_NET_SCH_DEFAULT=y
CONFIG_DEFAULT_FQ_CODEL=y

# Power Supply
CONFIG_BQ2597X=y

# Proximity
CONFIG_MIUS_PROXIMITY=y

# Touchscreen
CONFIG_TOUCHSCREEN_FOCALTECH=m
CONFIG_TOUCHSCREEN_XIAOMI_TOUCHFEATURE=m

# WiFi
CONFIG_CNSS_QCA6490=y

# CONFIG_IPC_LOGGING is not set
# CONFIG_IPC_LOG_MINIDUMP_BUFFERS is not set
# CONFIG_QTI_HW_MEMLAT_LOG is not s
